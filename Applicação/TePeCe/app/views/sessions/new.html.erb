<% provide(:title, "Dar entrada") %>
<h1></h1>
<legend>Sign In</legend>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
		<p> <p>
      <%= f.submit "Entrar", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>